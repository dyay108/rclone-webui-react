  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "check-prop-types": "^1.1.2",

    "@cfaester/enzyme-adapter-react-18": "^0.8.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.4.4",
    "fetch-mock": "^12.6.0",
    "react-dnd-test-backend": "^11.1.3",
    "react-dnd-test-utils": "^11.1.3",
    "react-scripts": "^5.0.1",
    "redux-mock-store": "^1.5.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:cov": "react-scripts test --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand",

    "eject": "react-scripts eject"
  },
  "bugs": {
    "url": "https://github.com/negative0/rclone-webui-react/issues"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],

  "overrides": {
    "nth-check": ">=2.0.1",
    "postcss": ">=8.4.31",
    "webpack-dev-server": ">=5.2.1",
    "jsonpath": "npm:jsonpath-plus@>=10.0.0",
    "rclone-api": {
      "axios": "$axios"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^undici$": "<rootDir>/src/__mocks__/undici.js",
      "^cheerio/lib/(.*)$": "<rootDir>/node_modules/cheerio/dist/commonjs/$1.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "src/**/**/.{js,jsx}",
      "!**/node_modules/**",
      "!**/*index.js",
      "!src/serviceWorker.js",
      "!src/polyfill.js"
    ]
  }
}
